{
    "file_path": "imu_data/your_dataset.csv",
    "result_dir": "results",
    "segment_length": 121,
    "resize_shape": "48,48",
    "scales": "logspace(0, log10(64), 32)",
    "sampling_period": 0.01,
    "batch_size": 64,
    "epochs": 100,
    "patience": 10,
    "n_iterations": 5,
    "target_columns": [
        ["hip_flexion_l_moment", "hip_flexion_l"],
        ["knee_angle_l_moment", "knee_angle_l"],
        ["ankle_angle_l_moment", "ankle_angle_l"]
    ],
    "run_tag": "incline_walk_paper",
    "version_tag": "v1.0",
    "num_workers": 4,
    "lr": 0.001,
    "seed": 42,
    "conv_hidden_imu": 64,
    "conv_hidden_cwt": 64,
    "lstm_hidden_sizes": "128,64,32",
    "tcn_hidden_channels": "64,64",
    "attention_heads": 4,
    "dropout_rate": 0.2,
    "use_attention": true,
    "use_fusion": true,
    "use_bitcn": true,
    "optimizer": "AdamW",
    "scheduler": "ReduceLROnPlateau",
    "weight_decay": 0.0001
}